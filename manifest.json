{
  "description": "Remove crap recommendation from Youtube.",
  "manifest_version": 3,
  "name": "No More Crap Recommendation",
  "version": "0.3.3",
  "icons": {
    "48": "icons/nope.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://m.youtube.com/*",
        "https://youtube.com/*"
      ],
      "js": [
        "src/removecrap.js"
      ]
    }
  ]
}